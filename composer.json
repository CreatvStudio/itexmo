{
    "name": "creatvstudio/itexmo",
    "description": "iTexMo API client for PHP.",
    "keywords": [
        "creatvstudio",
        "itexmo"
    ],
    "homepage": "https://github.com/creatvstudio/itexmo",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jeffrey Naval",
            "email": "jeff@creatvstudio.ph"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.16",
        "kitetail/zttp": "^0.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CreatvStudio\\Itexmo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CreatvStudio\\Itexmo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreatvStudio\\Itexmo\\Laravel\\ItexmoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
